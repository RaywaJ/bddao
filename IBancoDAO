import java.util.List;

/**
 *
 * @author Stefano
 */
public interface IBancoDAO {
    
    void SalvarPessoa (Pessoa p);
    
    void ExcluirPessoa (Pessoa p);
    
    List<Pessoa> ListarPessoa();
   
    void SalvarCarro (Carro c);
    
    void ExcluirCarro (Carro c);
    
    List<Carro> ListarCarro();
    
}
